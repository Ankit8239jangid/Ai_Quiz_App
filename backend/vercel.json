{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "../Frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/index.js",
      "dest": "index.js"
    },
    {
      "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|webp|ico|ttf|woff|woff2|json|txt))",
      "dest": "../Frontend/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "../Frontend/dist/index.html"
    }
  ]
}